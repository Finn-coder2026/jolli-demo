		"start": "next start -p 3034",
		"test": "vitest run --coverage"
	},
	"dependencies": {
		"@t3-oss/env-nextjs": "0.13.8",
		"jolli-common": "*",
		"lru-cache": "11.1.0",
		"next": "15.5.9",
		"pg": "8.16.3",
		"pino": "10.0.0",
		"pino-pretty": "13.1.2",
		"pino-roll": "4.0.0",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"sequelize": "6.37.7",
		"server-only": "0.0.1",
		"zod": "3.25.76"
	},
	"devDependencies": {
		"@types/node": "22.16.4",
		"open": "10.1.2",
		"@types/pg": "8.15.6",
		"@types/react": "19.1.8",
		"@types/react-dom": "19.1.6",
		"@vitejs/plugin-react": "4.5.2",
		"@vitest/coverage-v8": "3.2.3",
		"rimraf": "6.0.1",
		"typescript": "5.8.3",
		"vitest": "3.2.3"
	}
