{
	"name": "jolli-backend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "src/Main.ts",
	"scripts": {
		"all": "npm run clean && npm run build && npm run lint && npm run test && npm run package",
		"build": "npx tsgo",
		"clean": "rimraf dist/",
		"lint": "biome check --error-on-warnings",
		"lint:fix": "biome check --fix",
		"package": "vite build",
		"start": "vite",
		"test": "vitest run --coverage",
		"worker": "node --import tsx/esm src/worker/WorkerMain.ts",
		"worker:dev": "cross-env WORKER_MODE=true node --env-file=.env.local --import tsx/esm src/worker/WorkerMain.ts",
		"worker:build": "node build-worker.js",
		"worker:start": "node dist-worker/worker.js",
		"migrate:all-tenants": "node --import tsx/esm src/cli/MigrateSchemas.ts",
		"migrate:all-tenants:dry-run": "node --import tsx/esm src/cli/MigrateSchemas.ts --dry-run",
		"migrate:all-tenants:check": "node --import tsx/esm src/cli/MigrateSchemas.ts --check-only"
	},
	"dependencies": {
		"@ai-sdk/fireworks": "1.0.23",
		"@ai-sdk/openai": "2.0.52",
		"@aws-sdk/client-s3": "3.908.0",
		"@aws-sdk/client-ssm": "3.916.0",
		"@aws-sdk/client-sts": "3.943.0",
		"@aws-sdk/s3-request-presigner": "3.958.0",
		"@biomejs/js-api": "4.0.0",
		"@biomejs/wasm-nodejs": "2.3.8",
		"@langchain/anthropic": "0.3.31",
		"@langchain/aws": "0.1.15",
		"@langchain/core": "0.3.78",
		"@langchain/google-genai": "0.2.18",
		"@langchain/langgraph": "0.4.9",
		"@langchain/openai": "0.6.16",
		"@mdx-js/mdx": "3.1.0",
		"@octokit/auth-app": "8.1.1",
		"@octokit/plugin-throttling": "11.0.2",
		"@octokit/rest": "22.0.0",
		"@t3-oss/env-core": "0.13.8",
		"@types/cors": "2.8.19",
		"@types/express-session": "1.18.2",
		"@types/jsonwebtoken": "9.0.10",
		"ai": "5.0.70",
		"connect-session-sequelize": "8.0.2",
		"cookie-parser": "1.4.7",
		"cors": "2.8.5",
		"dotenv": "17.2.3",
		"express": "5.1.0",
		"express-session": "1.18.2",
		"grant": "5.4.24",
		"jolli-agent": "*",
		"jolli-common": "*",
		"jsonwebtoken": "9.0.2",
		"minimatch": "9.0.5",
		"morgan": "1.10.1",
		"nextra-generator": "*",
		"node-diff3": "3.2.0",
		"openai": "6.3.0",
		"pg": "8.16.3",
		"pg-boss": "11.1.1",
		"sequelize": "6.37.7",
		"smee-client": "4.3.1",
		"tiny-async-pool": "2.1.0",
		"wyhash": "1.0.0",
		"zod": "3.25.76",
		"zod-to-json-schema": "3.24.6"
	},
	"devDependencies": {
		"@electric-sql/pglite": "0.3.10",
		"@electric-sql/pglite-socket": "0.0.15",
		"@types/cookie-parser": "1.4.9",
		"@types/express": "5.0.3",
		"@types/morgan": "1.9.10",
		"@types/supertest": "6.0.3",
		"@types/tiny-async-pool": "2.0.3",
		"cross-env": "10.1.0",
		"esbuild": "0.25.10",
		"supertest": "7.1.4",
		"vite-plugin-node": "7.0.0"
	}
}
