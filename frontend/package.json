{
	"name": "jolli-frontend",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "index.html",
	"scripts": {
		"all": "npm run clean && npm run build && npm run lint && npm run test && npm run package",
		"build": "npx tsgo",
		"build:intlayer": "intlayer build",
		"clean": "rimraf dist/ && rimraf .intlayer",
		"generate:intlayer-mock": "npm run build:intlayer && tsx scripts/GenerateIntlayerMock.ts",
		"lint": "biome check --error-on-warnings && npx stylelint src/**/*.css",
		"lint:fix": "biome check --fix && npx stylelint --fix src/**/*.css",
		"package": "npm run build:intlayer && vite build",
		"preview": "vite preview --open --port 8034",
		"start": "vite",
		"test": "npm run generate:intlayer-mock && vitest run --coverage",
		"test:update": "npm run generate:intlayer-mock && vitest run --update"
	},
	"dependencies": {
		"@ai-sdk/react": "2.0.70",
		"@radix-ui/react-alert-dialog": "1.1.15",
		"@radix-ui/react-dialog": "1.1.15",
		"@radix-ui/react-dropdown-menu": "2.1.16",
		"@radix-ui/react-label": "2.1.8",
		"@radix-ui/react-select": "2.2.6",
		"@radix-ui/react-separator": "1.1.8",
		"@radix-ui/react-slot": "1.2.3",
		"@radix-ui/react-tabs": "1.1.13",
		"@radix-ui/react-tooltip": "1.2.8",
		"class-variance-authority": "0.7.1",
		"clsx": "2.1.1",
		"diff2html": "3.4.55",
		"highlight.js": "11.11.1",
		"intlayer": "7.0.7",
		"jolli-common": "*",
		"lucide-preact": "0.545.0",
		"lucide-react": "0.545.0",
		"markdown-to-jsx": "7.7.15",
		"preact": "10.27.2",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-intlayer": "7.0.7",
		"react-resizable-panels": "2.1.9",
		"tailwind-merge": "3.3.1"
	},
	"devDependencies": {
		"@preact/preset-vite": "2.10.2",
		"@tailwindcss/postcss": "4.1.14",
		"@testing-library/preact": "3.2.4",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"@vitejs/plugin-react": "5.0.4",
		"autoprefixer": "10.4.21",
		"baseline-browser-mapping": "2.9.4",
		"jsdom": "27.0.0",
		"postcss": "8.5.6",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"shadcn": "3.4.0",
		"stylelint": "16.25.0",
		"stylelint-config-css-modules": "4.5.1",
		"stylelint-config-standard": "39.0.1",
		"stylelint-order": "7.0.0",
		"tailwindcss": "4.1.14",
		"terser": "5.44.0",
		"vite-intlayer": "7.0.7"
	}
}
