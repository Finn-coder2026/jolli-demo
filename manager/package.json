{
	"name": "jolli-manager",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"all": "npm run clean && npm run build && npm run lint && npm run test",
		"build": "next build",
		"clean": "rimraf .next/ dist/",
		"dev": "node scripts/open-browser.mjs & next dev --turbopack -p 3034",
		"lint": "biome check --error-on-warnings",
